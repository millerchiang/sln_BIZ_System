@using prj_BIZ_System.Models
@model prj_BIZ_System.Models.Cluster_ViewModel
@{
    ViewBag.Title = "MessageClusterAddPrivate";
}

@section pagestyle{
    <link href="/stylesheets/sys.css" rel="stylesheet" />
    <link href="http://code.jquery.com/ui/1.11.4/themes/hot-sneaks/jquery-ui.css" rel="stylesheet">
}

@Styles.Render("~/Content/jqueryUI")
<div class="article_col1">
    @{
        Html.RenderAction("_ClusterMenuPartial", "Cluster");
    }
    <article class="article_col2">
        <h2 class="borderTitle2">聚落A</h2>
        <form class="form_box sys_searchbox">
            <h2>私人訊息</h2>
            <label for="" class="regis_col02L">對象</label>
            <div class="regis_col02R verticalslist">

                <input type="checkbox" class="inputCheckbox" id="busid01">
                <label for="busid01">欣欣紡織</label>
                <input type="checkbox" class="inputCheckbox" id="busid02">
                <label for="busid02">方方電子</label>
                <input type="checkbox" class="inputCheckbox" id="busid03">
                <label for="busid03">傅傳紡織</label>
                <input type="checkbox" class="inputCheckbox" id="busid04">
                <label for="busid04">安安科技</label>
                <input type="checkbox" class="inputCheckbox" id="busid05">
                <label for="busid05">昕傳科技</label>
            </div>
            <label for="" class="regis_col02L">主題</label>
            <input type="text" class="regis_col02R">
            <label for="" class="regis_col02L">內容</label>
            <textarea class="regis_col02R" name="" id="" placeholder="三百字以內" maxlength="300"></textarea>
            <label for="" class="regis_col02L">上傳附件</label>
            <div class="regis_col02R">
                <label class="addbu" id="upload" for="upexl">選取檔案</label>
                <input type="file" id="upexl" name="iupexl">
                <input type="text" id="upexl_name" name="upexl_name">
            </div>
            <div class="centerbox">
                <button type="submit" class="buinfor">確定新增</button>
                <a href="group_msg_manager.html" class="submitbu">返回訊息管理</a>
            </div>

        </form>
    </article>
</div>
@Scripts.Render("~/bundles/jqueryUI")