@using prj_BIZ_System.Models
@model ProductListModel

@{
    ViewBag.Title = "ProductList";
}
@section pagestyle{
    <link href="~/stylesheets/sys.css" rel="stylesheet" />
}
<div class="article_col1">
    <article class="article_col2">
        <section class="form_box w100 article_col1">
            <h2>@prj_BIZ_System.LanguageResource.Localization.getPropValue(Request.Cookies["_culture"], Model, "company")</h2>
            <div class="imgboxB">
                <img src="@(Url.Content(string.IsNullOrEmpty(Model.product_pic_site)?UploadHelper.defaultImgBig:ViewBag.productDir) + Model.product_pic_site)" alt="">
            </div>
            <div class="magb30">

                <span class="regis_col02L regis_col_bg2">
                    @prj_BIZ_System.LanguageResource.User.lb_p_category:
                </span>
                <span class="regis_col02R contentspan">
                    @prj_BIZ_System.LanguageResource.Localization.getPropValue(Request.Cookies["_culture"], Model, "product_category")
                </span>

                <span class="regis_col02L regis_col_bg2">
                    @prj_BIZ_System.LanguageResource.User.lb_product_name:
                </span>
                <span class="regis_col02R contentspan">
                    @prj_BIZ_System.LanguageResource.Localization.getPropValue(Request.Cookies["_culture"], Model, "product_name")
                </span>

                <span class="regis_col02L regis_col_bg2">
                    @prj_BIZ_System.LanguageResource.User.lb_p_model:
                </span>
                <span class="regis_col02R contentspan">
                    @prj_BIZ_System.LanguageResource.Localization.getPropValue(Request.Cookies["_culture"], Model, "model_no")
                </span>

                <span class="regis_col02L regis_col_bg2">
                    @prj_BIZ_System.LanguageResource.User.lb_p_info:
                </span>
                <p class="regis_col02R contentspan">
                    @prj_BIZ_System.LanguageResource.Localization.getPropValue(Request.Cookies["_culture"], Model, "product_info")
                </p>

                <span class="regis_col02L regis_col_bg2">
                    @prj_BIZ_System.LanguageResource.User.lb_p_patent:
                </span>
                <p class="regis_col02R contentspan">
                    @prj_BIZ_System.LanguageResource.Localization.getPropValue(Request.Cookies["_culture"], Model, "patent_or_winners")
                </p>

                <span class="regis_col02L regis_col_bg2">
                    @prj_BIZ_System.LanguageResource.User.lb_p_specification:
                </span>
                <p class="regis_col02R contentspan">
                    @prj_BIZ_System.LanguageResource.Localization.getPropValue(Request.Cookies["_culture"], Model, "specifications_or_other")
                </p>

            </div>
            <div class="centerbox magb30">
                @*<a href="../User/UserInfo?user_id=@(Model.user_id)#1" class="submitbu">@prj_BIZ_System.LanguageResource.User.lb_back_pdlist</a>*@
                <button type="button" class="submitbu" onclick="javascript:history.back()">@prj_BIZ_System.LanguageResource.User.lb_back</button>

            </div>
        </section>

    </article>
    @{
        Html.RenderAction("_HomeLeftPartial", "Home");
    }
</div>

<script>
    $(function () {
        init();
    });
    function init() {


    }
</script>