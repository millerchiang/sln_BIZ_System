@using prj_BIZ_System.Models
@model prj_BIZ_System.Models.Activity_ViewModel


@{
    ViewBag.Title = "賣家資訊列表";
}

<article class="form_box article_col1">
    <h2>賣家資訊列表</h2>
    <table class="edittable">
        <tr>
            <th>賣家帳號</th>
            <th>公司名稱</th>
            <!--<th>買主媒合需求</th>-->
        </tr>
        @foreach (ActivityRegisterModel sellerInfoModel in Model.activityregisterList)
        {
            <tr>
                <td><a href="/User/UserInfo?user_id=@sellerInfoModel.user_id">@sellerInfoModel.user_id</a></td>
                <td>@sellerInfoModel.company</td>
                @*<td>@buyerInfoModel.buyer_need</td>*@
            </tr>
        }
    </table>

</article>
