@using prj_BIZ_System.Models
@model prj_BIZ_System.Models.Match_ViewModel


@{
    ViewBag.Title = prj_BIZ_System.LanguageResource.User.lb_mm_result+ "(" + @prj_BIZ_System.LanguageResource.User.lb_buyer + ")";
}

@section pagestyle{
    <link href="~/stylesheets/schedule.css" rel="stylesheet" />
}

<div class="article_col1">
    @{
        Html.RenderAction("_NavSearchPartial", "User");
    }
    <article class="article_col2">
        <h2 class="borderTitle">
            @prj_BIZ_System.LanguageResource.User.lb_mm_result
        </h2>

        <section class="form_box w55">
            <h2>@prj_BIZ_System.LanguageResource.User.lb_buyer_chosen</h2>
            <ul class="comicon listlink">
                @foreach (MatchmakingAllModel matchmakingAllModel in Model.matchmakingAllList)
                {
                    <li>
                        <a href="@Url.Action("UserInfo","User",new {user_id = matchmakingAllModel.buyer_id})">@prj_BIZ_System.LanguageResource.Localization.getPropValue(Request.Cookies["_culture"], matchmakingAllModel, "company")</a>
                    </li>
                }
            </ul>
        </section>
        <section class="form_box w45">
            <h2>@prj_BIZ_System.LanguageResource.User.lb_buyer_matched</h2>
            <ul class="dmicon listlink">
                @foreach (MatchmakingScheduleModel matchmakingScheduleModel in Model.matchmakingScheduleList)
                {
                    <li>
                        <a href="@Url.Action("UserInfo","User",new {user_id = matchmakingScheduleModel.buyer_id})">@prj_BIZ_System.LanguageResource.Localization.getPropValue(Request.Cookies["_culture"], matchmakingScheduleModel, "company")</a>
                    </li>
                }
            </ul>
        </section>
    </article>
</div>
